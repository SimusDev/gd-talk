script/source = "extends SD_NodeConsoleCommands

func _ready() -> void:
	super()
	on_executed.connect(_on_command_executed)

func _on_command_executed(command:SD_ConsoleCommand) -> void:
	match command.get_code():
		\"info.send_message\":
			if command.get_arguments().size() == GDTalkInfo.get_method_argument_count(\"send\"):
				GDTalkInfo.send(command.get_value_as_string())
